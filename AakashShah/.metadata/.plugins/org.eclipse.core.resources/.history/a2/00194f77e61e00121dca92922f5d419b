
public class LongestSubsequence {
	
	private String data;
	private int len;
	
	private int[][] subdata;
	
	public LongestSubsequence(String data) {
		this.data = data;
		this.len = data.length();
		
		subdata = new int[len][len];
		initTable();
	}
	public void initTable() {
		for (int i = 0; i < len; ++i)
			subdata[i][i] = 1;
	}
	
	public static void main(String[] args) {
		

	}

}
